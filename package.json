{
  "name": "reshowcase",
  "version": "3.0.0",
  "bin": {
    "reshowcase": "./commands/reshowcase"
  },
  "scripts": {
    "build-bsb": "bsb -make-world",
    "start-bsb": "bsb -make-world -w",
    "clean-bsb": "bsb -clean-world",
    "start-reshowcase": "./commands/reshowcase ./example/Demo.bs.js",
    "start": "npm run clean-bsb && concurrently --names 'wp,bsb' -c 'bgBlue.bold,bgGreen.bold' 'npm run start-bsb' 'npm run start-reshowcase'"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "bloodyowl <mlbli@me.com>",
  "license": "MIT",
  "dependencies": {
    "html-webpack-plugin": "^5.3.1",
    "reason-react": ">=0.9.1",
    "webpack": "^5.27.0",
    "webpack-dev-server": "^3.11.2"
  },
  "devDependencies": {
    "bs-platform": "^7.1.1",
    "concurrently": "^6.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
